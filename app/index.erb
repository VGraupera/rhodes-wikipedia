<script language="JavaScript">
  // document.write(location.href);
  
  // this JS is here because iPhone redirects only work to top level
  
  if (location.href =='http://localhost:8080/app/index_erb.iseq?history=true') {
    window.location ='/app/WikipediaPage/history';
  }
  else if (/\/app\/index_erb.iseq\?show_old=true/.test(location.href))
  {
    // cut off leading url and just pass on params
    window.location ='/app/WikipediaPage?'+location.href.substring(41);
  }
  else {
    window.location ='/app/WikipediaPage';
  }
    
</script>

<ul id="home" selected="true" title="Wikipedia">  
  <li><a href="WikipediaPage/index?search=::Home">Go</a></li>
</ul>

